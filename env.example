# API Keys
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here
EXA_API_KEY=your_exa_key_here

# Database
DATABASE_URL="postgresql://app_user:app_password@localhost:5432/app_db?sslmode=disable"

# Application Configuration
NODE_ENV=development
PORT=4111
HOSTNAME=0.0.0.0

# CORS Configuration
CORS_ORIGINS="http://localhost:4111,http://0.0.0.0:4111"

# Authentication
MASTRA_JWT_SECRET=your_jwt_secret_here_make_it_long_and_random
MASTRA_APP_PASSWORD=your_app_password_here

# Deployment (set automatically in CI/CD)
DEPLOYMENT_ID=local
ENVIRONMENT=development

# Optional: Redis (if using caching)
REDIS_URL=redis://localhost:6379

# Optional: Google Cloud Storage (for production)
# GCS_BUCKET=your-bucket-name
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json
